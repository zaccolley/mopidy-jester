    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "css-loader": "^0.18.0",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.4",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.4",
